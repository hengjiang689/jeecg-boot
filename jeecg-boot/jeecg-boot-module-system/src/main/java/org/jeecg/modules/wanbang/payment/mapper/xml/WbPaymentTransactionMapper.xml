<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.wanbang.payment.mapper.WbPaymentTransactionMapper">

    <select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.wanbang.payment.entity.WbPaymentTransaction">
		SELECT *
		FROM  wb_payment_transaction
		WHERE
			 out_trade_no = #{outTradeNo} 	</select>
</mapper>
